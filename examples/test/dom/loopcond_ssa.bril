@main {
.entry:
  x_1: int = const 0;
  i_1: int = const 0;
  one: int = const 1;

.loop:
  x_2: int = phi x_1 x_5 .entry .endif;
  i_2: int = phi i_1 i_3 .entry .endif;
  max: int = const 10;
  cond_1: bool = lt i_2 max;
  br cond_1 .body .exit;

.body:
  mid: int = const 5;
  cond_2: bool = lt i_2 mid;
  br cond_2 .then .endif;

.then:
  x_3: int = add x_2 one;
  jmp .endif;

.endif:
  x_4: int = phi x_2 x_3 .body .then;
  factor: int = const 2;
  x_5: int = mul x_4 factor;
  i_3: int = add i_2 one;
  jmp .loop;

.exit:
  print x_2;
}